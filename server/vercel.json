{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
    "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://travel-experience-frontend.vercel.app"
        }
      ]
}
